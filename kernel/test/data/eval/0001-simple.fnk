;;;; -*- mode: finkel -*-

(let ((:: x y Code)
      (= x '(a b c))
      (= y '(foo a b c))

      (:: cdr (-> Code Code))
      (= cdr (LForm (L l (List (: _ rest))))
        (LForm (L l (List rest))))

      (:: fib (-> Int Int))
      (= fib n
        (if (< n 2)
            n
            (+ (fib (- n 1))
               (fib (- n 2))))))

  (all id [(== 'foo 'foo)
           (== x (cdr y))
           (== (fib 10) 55)]))
